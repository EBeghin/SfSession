{% extends 'base.html.twig' %}

{% block title %}Liste des stagiaires{% endblock %}

{% block body %}
    <h1>Listes des stagiaires</h1><br>

    {% for stagiaire in stagiaires %}
        <a href="{{ path('showDetail_stagiaire', {'id': stagiaire.id}) }}"> {{ stagiaire }} </a>
        <a class = "btn btn-primary" href="{{ path('edit_stagiaire', {'id': stagiaire.id}) }}">Edition</a>
        <a class = "btn btn-primary" href="{{ path('delete_stagiaire', {'id': stagiaire.id}) }}">Supprimer</a><br><br>
    {% endfor %}

{% endblock %}
